{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Pacman: smallGrid",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/pacman.py",
      "cwd": "${workspaceFolder}/src",
      "args": [
        "-p",
        "MDPAgent",
        "-l",
        "smallGrid",
        "-n",
        "1",
        "-q"
      ],
      "console": "integratedTerminal"
    },
